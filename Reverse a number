import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
         int input= sc.nextInt();
         int temp =0;
         int reverse =0;
         while(input!=0){
             temp=input%10;
             reverse=reverse*10+temp;
             input=input/10;
             
         }
         System.out.println(reverse);
    }
}
